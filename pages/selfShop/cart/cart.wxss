/* 字体图标 */

@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'), url(data:application/font-woff;charset=utf-8;base64,) format('woff'), url('iconfont.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
}

@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'), url(data:application/font-woff;charset=utf-8;base64,) format('woff'), url('iconfont_1.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
}

.iconfont {
    font-family: "iconfont" !important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.icon-checked:before {
    content: "\e605";
    color: red;
}

.icon-dianpu:before {
    content: "\e666";
}

.icon-shanchu:before {
    content: "\e61c";
}

page {
    width: 100%;
    height: 100%;
    background-color: #F2F2F2;
}

.shopping-cart {
    width: 100%;
    height: 100%;
    background-color: #F2F2F2;
}


/*  添加的各个商品列表 */

.shopping-cart .cart-goods {
    margin: 20rpx 0;
}


/* 店铺分类 */

.shopping-cart .cart-goods .goods-classify {
    /* background-color: #fff; */
    margin-top: 20rpx;
}


/* 店铺的商品分类 */

.cart-goods .goods-classify .goods-classify-detail {
    width: 100%;
    /* height: 400rpx; */
    padding: 20rpx 10rpx;
    box-sizing: border-box;
    margin-top: 20rpx;
    background-color: #fff;
}

.goods-classify .goods-classify-detail .classify-detail-title {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin: 15rpx 0 15rpx 20rpx;
}

.goods-classify .goods-classify-detail .classify-detail-title .title-left {
    display: flex;
    justify-content: space-around;
    align-items: center;
    font-size: 32rpx;
}


/* 全选按钮 */

.ckeckAll {
    width: 38rpx;
    height: 38rpx;
    border-radius: 50%;
    border: 1px solid #ccc;
    font-size: 40rpx;
    flex-shrink: 0;
}

.goods-classify-detail .classify-detail-title .title-left .icon-checked {
    font-size: 42rpx;
}


/* 店铺图标 */

.goods-classify-detail .classify-detail-title .title-left .icon-dianpu {
    width: 48rpx;
    height: 48rpx;
    font-size: 48rpx;
    margin: 0 20rpx;
}


/* 删除按钮 */

/* .goods-classify .goods-classify-detail .classify-detail-title .icon-shanchu {
    width: 55rpx;
    height: 55rpx;
    font-size: 55rpx;
} */


/* 右侧内容 */

.goods-classify-detail .classify-detail-content {
    display: flex;
    justify-content: space-around;
    align-items: center;
    margin-top: 30rpx;
    margin-right: 15rpx;
}


/* 单选图标 */

.radio-select {
    width: 38rpx;
    height: 38rpx;
    border-radius: 50%;
    border: 1px solid #ccc;
    font-size: 40rpx;
    flex-shrink: 0;
    margin: 0 10rpx;
}

.goods-classify-detail .classify-detail-content .icon-checked {
    font-size: 42rpx;
    margin: 0 10rpx;
    /* flex-shrink: 0; */
}

.goods-classify-detail .classify-detail-content .detail-content-right {
    flex: 1;
    display: flex;
    justify-content: flex-start;
    align-items: center;
}


/* 商品图片 */

.classify-detail-content .detail-content-right .content-img {
    width: 176rpx;
    height: 176rpx;
    flex-shrink: 0;
}

.classify-detail-content .detail-content-right .content-img image {
    width: 100%;
    height: 100%;
    border-radius: 10rpx;
}


/* 商品说明 */

.classify-detail-content .detail-content-right .content-explain {
    color: #333333;
    font-size: 26rpx;
    margin-left: 20rpx;
    flex-shrink: 1;
    flex: 1;
}


/* 商品名称 */

.classify-detail-content .detail-content-right .content-explain .content-explain-name {
    display: -webkit-box;
    overflow: hidden;
    text-overflow: ellipsis;
    word-wrap: break-word;
    white-space: normal !important;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}


/* 颜色和数量选择 */

.detail-content-right .content-explain .colorOrNumber {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin: 15rpx 0;
}

.detail-content-right .content-explain .colorOrNumber .content-explain-color {
    /* width: 210rpx; */
    /* border: 1px solid #000; */
    height: 60rpx;
    display: flex;
    background-color: #f9f9f9;
    border-radius: 10rpx;
    text-align: center;
    line-height: 60rpx;
}

/* .content-explain .content-explain-color text:last-child {
    display: inline-block;
    width: 10rpx;
    height: 10rpx;
    border-right: 1px solid #CCCCCC;
    border-bottom: 1px solid #CCCCCC;
    transform: rotate(45deg);
    /* margin-bottom: 5rpx; 
} */

/* 商品数量加减 */

.detail-content-right .content-explain .colorOrNumber .select-num {
    width: 150rpx;
    height: 42rpx;
    border-radius: 10rpx;
    border: 1px solid #EDEDED;
    display: flex;
    justify-content: space-around;
    align-items: center;
}

.colorOrNumber .select-num .subtract,
.colorOrNumber .select-num .plus {
    color: #ECECEC;
    flex: 2;
    text-align: center;
    color: #333;
}

.colorOrNumber .select-num .number {
    flex: 3;
    text-align: center;
    border-left: 1px solid #ECECEC;
    border-right: 1px solid #ECECEC;
    color: #333;
}


/* 商品价格 */

.detail-content-right .content-explain .content-explain-price {
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.detail-content-right .content-explain .content-explain-price .price {
    color: #FF0000;
    font-size: 34rpx;
}

.detail-content-right .content-explain .content-explain-price .icon-shanchu {
    width: 55rpx;
    height: 55rpx;
    font-size: 55rpx;
}



/* 购物车空白 */
.shopping-cart .blank {
    width: 100%;
    /* height: 58%; */
    margin: 20rpx 0;
    background-color: #fff;
    text-align: center;
    overflow: hidden;
}

.shopping-cart .blank .blank-img {
    height: 350rpx;
    width: 350rpx;
    margin: 50rpx auto;
}

.shopping-cart .blank .blank-img image {
    width: 100%;
    height: 100%;
}

.shopping-cart .blank .blank-hint {
    color: #999999;
    font-size: 38rpx;
    margin:30rpx 0;
}

.shopping-cart .blank .blank-indicate {
    display: flex;
    justify-content: space-around;
    align-items: center;
    width: 80%;
     margin: 60rpx auto;
}

.blank .blank-indicate text  {
    width: 220rpx;
    height: 70rpx;
    border: 1px solid #FF2C55;
    color: #FF2C55;
    background-color: #fff;
    text-align: center;
    line-height: 70rpx;
    font-size: 38rpx;
}

/* 看了又看 */

.shopping-cart .lookAndSee {
    width: 100%;
    padding: 0 25rpx;
    box-sizing: border-box;
    background-color: #fff;
    margin-bottom: 120rpx;
}

.shopping-cart .lookAndSee .LAS-title {
    height: 100rpx;
    width: 100%;
    text-align: center;
    font-size: 30rpx;
    color: #6C6C6C;
    line-height: 100rpx;
}

.res-list{
    width:100%;
    display: flex;
    justify-content: space-between;
    flex-wrap:wrap;
    padding: 0 26rpx;
    box-sizing: border-box;
  }
  .res-list .res-list-item{
    width:48%;
    margin-top:26rpx;
    border:1px solid #eee;
    border-radius:10px;
    overflow: hidden;
  }
  .res-list .res-list-item .res-list-item-msg{
    padding:0rpx 11rpx 13rpx;
    min-width: 0;
    flex: 1; 
  }
  .res-list .res-list-item .res-list-item-msg .res-list-item-msg-title{
    color:#333;
    font-size: 28rpx;
    padding-top:20rpx;
    padding-bottom:5rpx;
    overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  }
  .res-list .res-list-item .res-list-item-msg .res-list-item-msg-label{
    color:#999;
    font-size: 20rpx;
    overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  padding-bottom:5rpx;
  }
  .res-list .res-list-item .res-list-item-msg .res-list-item-msg-price{
    color:#FF3E3E;
    font-size: 28rpx;
    font-weight: bold;
  }
  .res-list .res-list-item .res-list-item-msg .res-list-item-msg-price-title{
      text-align: center;
    color:#FF4444;
    font-size: 20rpx;
    border:1px solid #FF4444;
    padding:3rpx 8rpx;
    position: relative;
    top:-2rpx;
  }
  .res-list .res-list-item .res-list-item-msg .res-list-item-msg-bottom{
    display: flex;
    justify-content: space-between;
    color:#999999;
    font-size: 20rpx;
    padding-top:5rpx;
  }
  .res-list .res-list-item .res-list-item-msg .res-list-item-msg-bottom .res-list-item-msg-marketPrice{
    text-decoration:line-through;
  }

/* 去结算 */

.shopping-cart .clearing {
    width: 100%;
    height: 120rpx;
    position: fixed;
    bottom: 0;
    left: 0;
    display: flex;
    justify-content: space-around;
    align-items: center;
    background-color: #fff;
}

.clearing .selectAll {
    height: 100%;
    width: 33%;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 32rpx;
}

.clearing .selectAll .all {
    display: inline-block;
    width: 40rpx;
    height: 40rpx;
    border-radius: 50%;
    border: 1px solid #ccc;
    font-size: 40rpx;
    margin-right: 10rpx;
}

.clearing .selectAll .icon-checked {
    font-size: 42rpx;
    margin-right: 10rpx;
}

.clearing .total {
    height: 100%;
    width: 33%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    text-align: right;
    padding-right: 30rpx;
}

.clearing .total .totle-price {
    color: #FF060D;
    font-size: 32rpx;
}

.clearing .total .freight {
    font-size: 26rpx;
    color: #888888;
}

.clearing .settle-accounts {
    height: 120rpx;
    width: 33%;
    background-color: #FF2B54;
    color: #fff;
    font-size: 32rpx;
    text-align: center;
    line-height: 120rpx;
}