.my-container{
    background: #f8f8f8;
}
.login-btn{
    font-size:24rpx;
    background: #FE4B4B;
}
/* 个人信息 */
.info-block{
    width:100%;
    height:343rpx;
    padding:0 26rpx;
    display: flex;
    box-sizing:border-box;
    flex-wrap:wrap;
    position: relative;
}
.info-block-banner{
    position: absolute;
    left:0;
    top:0;
    z-index: 1;
}
.info-img,.info-message,.info-code-block{
    position: relative;
    z-index: 2;
}
.info-block .info-img{
    width:144rpx;
    height:144rpx;
    border-radius: 50%;
    overflow: hidden;
    border:1px solid #fff;
    margin-top:55rpx;
    margin-left:19rpx;
}
.info-block .info-message{
    flex: 1;
    color:#fff;
    margin-top:80rpx;
    margin-left:20rpx;
    padding-right:65rpx;
}
.info-block .info-message .name-and-vip{
    display: flex;
    justify-content: space-between;
}
.info-block .info-message .name-and-vip .info-vip{
    display: flex;
    color:#FFEBB7;
    font-size: 24rpx;   
    background: #333333;
    border-radius: 20px;
    padding:10rpx 30rpx;
}
.info-block .info-message .info-sign text{
    width:100%;
    display: -webkit-box;
    overflow: hidden;
    -webkit-line-clamp:2;
    -webkit-box-orient: vertical;
    font-size: 20rpx;
    margin-top:20rpx;
}
.info-block .info-code-block{
    display: flex;
    width:100%;
    padding:0 20rpx;
    justify-content: space-between;
    font-size:28rpx;
    color:#fff;
    margin-top:-15px;
}
.info-block .info-code-block .info-btn{
    font-size: 18rpx;
    border-radius: 20px;
    padding:1px 5px;
    position: relative;
    top:-1px;
    border:1px solid #fff;
}
/* 详情 */
.center-block{
    padding:0 30rpx;
    margin-top:-40rpx;
    position: relative;
    z-index: 2;
}
.center-block .center-all-block{
    border-radius:10px;
    box-shadow:0px 3px 7px 0px rgba(0, 0, 0, 0.16);
    overflow: hidden;
    margin-bottom:17rpx;
    background: #fff;
}
/* 余额 */
.center-block .center-balance .center-adv{
    display: flex;
    justify-content: space-between;
    color:#FFEBB7;
    font-size:24rpx;
    background: #333333;
    padding:25rpx 20rpx;
}
.center-block .center-balance .center-cash{
    display: flex;
    justify-content: space-between;
    padding:38rpx 25rpx;
    border-bottom:1px solid #ddd;
}
.center-block .center-balance .center-cash .center-cash-can{
    color:#323333;
    font-size:28rpx;
}
.center-block .center-balance .center-cash .center-cash-can .center-cash-num{
    color:#FE4B4B;
    font-size:34rpx;
}
.center-block .center-balance .center-cash .center-cash-btn{
    background: #FE4B4B;
    border-radius:24px;
    font-size:24rpx;
    color:#fff;
    padding:5px 10px;
}
.center-block .center-balance .center-profit{
    display: flex;
    justify-content: space-between;
    padding:31rpx 47rpx;
    color:#323333;
    font-size:24rpx;
    text-align: center;
}
.center-block .center-balance .center-profit .center-profit-num{
    color:#FE4B4B;
    font-size:34rpx;
    padding-bottom:10rpx;
}
.shop-order .shop-order-detail{
    display: flex;
    justify-content: space-between;
    padding:32rpx 26rpx;
    color:#323333;
    font-size:28rpx;
    border-bottom:1px solid #ddd;
}
.shop-order .shop-order-detail .shop-order-detail-router{
    color:#666666;
    font-size:24rpx;
}
.shop-order .shop-order-state{
    display: flex;
    justify-content: space-between;
    padding:28rpx 28rpx 30rpx;
    color:#323333;
    font-size:24rpx;
}
.shop-order .shop-order-state>view{
    width:25%;
    text-align: center;
    position: relative;
}
.shop-order .shop-order-state .shop-order-state-num{
    position: absolute;
    top:-9px;
    right:20px;
    color:#FF3931;
    font-size: 12rpx;
    border-radius: 50%;
    width:20rpx;
    height:20rpx;
    line-height: 20rpx;
    border:1px solid #FF3931;
    text-align: center;
}
.other-functions .other-functions-block{
    display: flex;
    justify-content: space-between;
    padding:28rpx 28rpx 28rpx;
    color:#666;
    font-size:24rpx;
}
.other-functions .other-functions-block>view{
    width:25%;
    text-align: center;
    position: relative;
}
.other-functions .other-functions-block .other-functions-num{
    position: absolute;
    top:-9px;
    right:20px;
    color:#FF3931;
    font-size: 12rpx;
    border-radius: 50%;
    width:20rpx;
    height:20rpx;
    line-height: 20rpx;
    border:1px solid #FF3931;
    text-align: center;
}
.other-block-item{
    display: flex;
    color:#323333;
    font-size: 24rpx;
    padding:30rpx 0;
    border-bottom:1px solid #EEEEEE;
}
.other-block-item text{
    width:30rpx;
    color:#666;
}
.other-block-item .other-block-item-title{
    flex: 1;
}
.other-block-item-block{
    padding:0 26rpx;
}